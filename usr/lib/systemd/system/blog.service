[Unit]
Description=KC Blog web application

[Service]
Type=forking
User=capistrano
Group=capistrano
WorkingDirectory=/u/apps/blog/current
ExecStart=/u/apps/blog/current/blog start
PIDFile=/u/apps/blog/shared/pids/server.pid

# Timeout for start up/shut down
TimeoutSec=300

[Install]
WantedBy=multi-user.target
